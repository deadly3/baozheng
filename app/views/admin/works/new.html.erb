<table class="table table-boldered">
  <% @works.each do |work| %>
  <tr>
    <td>
      <%= work.title %>
    </td>
    <td>
      <%= work.description %>
    </td>
    <td>
      <%= work.price %>
    </td>
    <td>
      <%= work.image %>
    </td>
    <td>
      <%= job.created_at %>
    </td>
  </tr>
  <% end %>
</table>
